        "deploy-commands": "node build/scripts/deploy-commands.js",
        "end-season": "ts-node scripts/end-season.ts"
    },
    "dependencies": {
        "@discordjs/rest": "2.2.0",
        "@types/node-cron": "3.0.11",
        "async-csv": "2.1.3",
        "discord-api-types": "0.37.67",
        "discord.js": "14.14.1",
        "dotenv": "16.4.1",
        "emoji-regex": "10.3.0",
        "math-z-score": "1.0.0",
        "mongoose": "8.1.1",
        "node-cron": "3.0.3",
        "parse-duration": "1.1.0",
        "simple-elo-rating": "1.0.0",
        "uuid": "9.0.1"
    },
    "devDependencies": {
        "@types/jest": "29.5.12",
        "@types/uuid": "9.0.8",
        "eslint": "8.56.0",
        "jest": "29.7.0",
        "mongodb-memory-server": "9.1.6",
        "nodemon": "3.0.3",
        "ts-jest": "29.1.2",
        "ts-mockito": "2.6.1",
        "ts-node": "10.9.2",
        "typescript": "5.3.3"
    }
