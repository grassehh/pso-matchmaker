{
    "name": "pso-matchmaker",
    "version": "1.6.0",
    "types": "build/bot/src/index.d.ts",
    "scripts": {
        "build": "tsc --build",
        "build-bot": "tsc --build bot/tsconfig.json",
        "build-scripts": "tsc --build scripts/tsconfig.json",
        "build-migrations": "tsc --build migrations/tsconfig.json",
        "clean": "tsc --build --clean",
        "test": "jest ./bot/test/*",
        "test:debug": "jest --verbose --runInBand ./bot/test/*",
        "dev": "npx nodemon bot/src/bot.ts",
        "start": "node index.js",
        "deploy-commands": "node build/scripts/deploy-commands.js",
        "end-season": "node build/scripts/end-season.js"
    },
    "dependencies": {
        "@discordjs/rest": "1.2.0",
        "@types/node-cron": "3.0.4",
        "async-csv": "2.1.3",
        "discord-api-types": "0.37.8",
        "discord.js": "14.5.0",
        "dotenv": "16.0.2",
        "emoji-regex": "10.1.0",
        "math-z-score": "1.0.0",
        "mongoose": "6.6.0",
        "node-cron": "3.0.2",
        "simple-elo-rating": "1.0.0",
        "uuid": "9.0.0"
    },
    "devDependencies": {
        "@types/jest": "28.1.8",
        "@types/uuid": "8.3.4",
        "eslint": "8.23.0",
        "jest": "28.1.3",
        "mongodb-memory-server": "8.9.1",
        "ts-jest": "28.0.8",
        "ts-mockito": "2.6.1",
        "ts-node": "10.9.1",
        "typescript": "4.8.4",
        "nodemon": "2.0.19"
    }
}