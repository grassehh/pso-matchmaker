{
    "name": "pso-matchmaker",
    "version": "1.6.0",
    "types": "build/bot/src/index.d.ts",
    "scripts": {
        "build": "tsc --build",
        "build-bot": "tsc --build bot/tsconfig.json",
        "build-scripts": "tsc --build scripts/tsconfig.json",
        "build-migrations": "tsc --build migrations/tsconfig.json",
        "clean": "tsc --build --clean",
        "test": "jest ./bot/test/*",
        "test:debug": "jest --verbose --runInBand ./bot/test/*",
        "dev": "npx nodemon bot/src/bot.ts",
        "start": "node index.js",
        "deploy-commands": "node build/scripts/deploy-commands.js",
        "end-season": "ts-node scripts/end-season.ts"
    },
    "dependencies": {
        "@discordjs/rest": "2.0.1",
        "@types/node-cron": "3.0.10",
        "async-csv": "2.1.3",
        "discord-api-types": "0.37.60",
        "discord.js": "14.13.0",
        "dotenv": "16.3.1",
        "emoji-regex": "10.3.0",
        "math-z-score": "1.0.0",
        "mongoose": "7.6.3",
        "node-cron": "3.0.2",
        "parse-duration": "1.1.0",
        "simple-elo-rating": "1.0.0",
        "uuid": "9.0.1"
    },
    "devDependencies": {
        "@types/jest": "29.5.6",
        "@types/uuid": "9.0.6",
        "eslint": "8.52.0",
        "jest": "29.7.0",
        "mongodb-memory-server": "9.0.1",
        "nodemon": "3.0.1",
        "ts-jest": "29.1.1",
        "ts-mockito": "2.6.1",
        "ts-node": "10.9.1",
        "typescript": "5.2.2"
    }
}
